
.honor {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.honor .header {
  width: 100%;
  padding: 18rpx 0 50rpx;
  border-radius: 0 0 40rpx 40rpx;
  background: #46d7b6;
}
.honor .header .title {
  width: 180rpx;
  height: 75rpx;
}
.honor .medal-box {
  flex: 1;
  display: flex;
  padding: 30rpx;
  overflow-y: auto;
  justify-content: space-between; 
  flex-wrap: wrap;
}

.medal-item{
  width: 200rpx;
  height: 280rpx;
}
.medal-item image {
  width: 100%;
  height: 100%;
}

