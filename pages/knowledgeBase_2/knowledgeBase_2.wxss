/* pages/knowledgeBase_2/knowledgeBase_2.wxss */
page {
  width: 100%;
  height: 100%;
}
.page_box {
  width: 100%;
  height: 100%;
  background: #fefefe;
}
.page_list {
  width: 652rpx;
  margin: 50rpx auto 0;
}
.file_name {
  width: 100%;
  background: #fefefe;
  margin-top: 20rpx;
  border-radius: 20rpx;
  padding: 22rpx 10rpx;
  box-shadow: 0px 4px 16px -6px rgba(0, 0, 0, 0.16);
  text-align: center;
  line-height: 64rpx;
  font-size: 36rpx;
  color: #46d7b6;
}

.file_name.title {
  font-size: 38rpx;
  color: #605e5f;
}