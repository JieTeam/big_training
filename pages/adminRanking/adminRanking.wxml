<view class="bj-page">
  <view class="page__header">
    <view class="title">{{userInfo.fullRegionName}}</view>
    <view class="tab">
      <view 
        wx:for="{{rankingTabData}}" 
        wx:key="index"
        data-item="{{item}}"
        bindtap="handleTab"
        class="tab__item {{item.checked ? 'checked' : '' }}">
        {{item.title}}
      </view>
    </view>
  </view>
  <view class="page__content">
    <ranking-table tableHead="{{tableHead}}" cloumn="{{personClomn}}" data="{{personData}}"></ranking-table>
  </view>
</view>
