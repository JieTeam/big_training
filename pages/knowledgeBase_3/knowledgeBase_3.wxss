/* pages/knowledgeBase_3/knowledgeBase_3.wxss */
page {
  width: 100%;
  height: 100%;
}
.page_box {
  width: 100%;
  height: 100%;
}
.file_name {
  width: 612rpx;
  padding: 20rpx;
  background: #fefefe;
  margin: 35rpx auto 30rpx;
  border-radius: 20rpx;
  box-shadow: 0px 0px 18px -6px rgba(0, 0, 0, 0.3);
  text-align: center;
  line-height: 52rpx;
  font-size: 36rpx;
  color: #46d7b6;
}
.rules_text {
  width: 100%;
  padding: 0 45rpx 80rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  white-space: pre-line;
}
.paragraph {
  margin-bottom: 30rpx;
}