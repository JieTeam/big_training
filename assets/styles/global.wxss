

ul,
li,
ol,
dl {
    list-style: none;
}

.pa {
    position: absolute;
}

.pr {
    position: relative;
}

.pf {
    position: fixed;
}

.df {
    display: flex;
}

.flexCol {
    flex-direction: column;
}

.justBet {
    justify-content: space-between;
}

.justAround {
    justify-content: space-around;
}

.justCen {
    justify-content: center;
}

.flexWarp {
    flex-wrap: wrap;
}

